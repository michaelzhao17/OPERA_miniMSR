/ outer airblock
SKETCH OBJECT=BLOCK +COMPLETE
PREVIEW OPTION=END REDISPLAY=NO |  BLOCK Name='AirBlockOut' X0=0.4 Y0=0.4 Z0=0.4 X1=-0.4 Y1=-0.4 Z1=-0.4 MATERIALLABEL='Air' LEVEL=#air_red_DSL
/ rename
FILTER TYPE=BODY
PREVIEW OPTION=PICK | PICK OPTION=ADD PROPERTY=UniqueName LABEL='AirBlockOut'
PREVIEW OPTION=ACCEPT REDISPLAY=NO |  RENAME NAME='AirBlockOut' UNIQUENAME='AirBlockOut'
/ mesh parameters
PREVIEW OPTION=PICK | PICK OPTION=ADD PROPERTY=UniqueName LABEL='AirBlockOut'
PREVIEW OPTION=ACCEPT REDISPLAY=NO |  CELLDATA OPTION=MODIFY MATERIALLABEL='Air' POTENTIAL=Reduced ELEMENTTYPE=Linear LEVEL=#air_red_DSL SIZE=#air_red_MES NORMALTOL=#air_MAbE ELEMSHAPEPREF=NONE

/ inner airblock
SKETCH OBJECT=BLOCK +COMPLETE
PREVIEW OPTION=END REDISPLAY=NO |  BLOCK Name='AirBlockIn' X0=0.3 Y0=0.3 Z0=0.3 X1=-0.3 Y1=-0.3 Z1=-0.3 MATERIALLABEL='Air' LEVEL=#air_red_DSL
/ rename
